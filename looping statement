let numbers = [1, 2, 3, 4, 5]

for number in numbers {
    print(number)
}

// Iterating through a range
for i in 1...5 {
    print(i)
}

//Iterating through a dictionary
let person = ["name": "Alice", "age": 30]
for (key, value) in person {
    print("\(key): \(value)")
}

var counter = 0

while counter < 5 {
    print(counter)
    counter += 1
}

var counter = 0

repeat {
    print(counter)
    counter += 1
} while counter < 5
